
@{
    ViewBag.Title = "Index";
}

<h2>Overview</h2>

@Html.ActionLink("Logout", "Signout", "Account")

<table align="center">
    @{ 
        foreach (var account in ViewBag.AccountsList)
        {
            <tr>
                <td>@account.name</td>
                <td>@account.value</td>
            </tr>
        }
    }
</table>